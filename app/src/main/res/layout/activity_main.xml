<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:padding="5dp">

        <Button
            android:id="@+id/btn_fetch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:onClick="fetch"
            android:text="@string/fetch"
            tools:ignore="OnClick,UsingOnClickInXml" />

        <EditText
            android:id="@+id/edit_query"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="3dp"
            android:layout_toStartOf="@id/btn_fetch"
            android:hint="@string/enter_your_url"
            android:singleLine="true"
            android:inputType="text"
            android:importantForAutofill="no" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/head">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="16dp"
            tools:ignore="ScrollViewSize">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:baselineAligned="false">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:padding="3dp">

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="streamvid"
                        android:text="@string/streamvid"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="mp4upload"
                        android:text="@string/mp4upload"
                        tools:ignore="OnClick,UsingOnClickInXml"/>


                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="mediafire"
                        android:text="@string/mediafire"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="vk"
                        android:text="@string/vk"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="youtube"
                        android:text="@string/youtube"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="vidozafiles"
                        android:text="@string/vidoza"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="sendvid"
                        android:text="@string/sendvid"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="filerio"
                        android:text="@string/filerio"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="fourshared"
                        android:text="@string/_4shared"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="vudeo"
                        android:text="@string/vudeo"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="doodstream"
                        android:text="@string/doodstream"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="mixdrop"
                        android:text="@string/mixdrop"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="bitchute"
                        android:text="@string/bitchute_btn"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="deadlyblogger"
                        android:text="@string/deadlyblogger_btn"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="googleusercontent"
                        android:text="@string/googleusercontent_btn"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="yodbox"
                        android:text="@string/yobox_btn"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="streamhide"
                        android:text="@string/streamhide"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="eplayvid"
                        android:text="@string/eplayvidbtn"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:padding="3dp">

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="gphotos"
                        android:text="@string/google_photos"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="okru"
                        android:text="@string/ok_ru"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="dailymotion"
                        android:text="@string/dailymotion"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="vidbm"
                        android:text="@string/vidbm"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="streamtape"
                        android:text="@string/streamtape"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="amazon"
                        android:text="@string/amazon_drive"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="streamsb"
                        android:text="@string/streamsbz"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="aparat"
                        android:text="@string/aparat_btn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="archive"
                        android:text="@string/archive_btn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="brighteon"
                        android:text="@string/brighteon_btn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="hdvid"
                        android:text="@string/hdvid_vidhdthe_btn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="voesx"
                        android:text="@string/voesx_btn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="vidmoly"
                        android:text="@string/vidmolybtn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="upstream"
                        android:text="@string/upstream_btn"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="streamlare"
                        android:text="@string/streamlare"
                        tools:ignore="OnClick,UsingOnClickInXml" />

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="cloudvideo"
                        android:text="@string/cloudvideo"
                        tools:ignore="OnClick,UsingOnClickInXml"/>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:onClick="midian"
                        android:text="@string/midian_appboxes_avi_not_supported"
                        tools:ignore="OnClick,UsingOnClickInXml"/>
                </LinearLayout>

            </LinearLayout>

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:gravity="center"
                android:onClick="dev"
                android:text="@string/developed_by_inside4ndroid_studios_ncredit_to_khun_htetz_naing_for_Jresolver"
                tools:ignore="OnClick,UsingOnClickInXml" />

        </LinearLayout>
    </ScrollView>

</RelativeLayout>